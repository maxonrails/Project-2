<%= form_for @post, remote: true do |f| %>

<div>
     <%= f.label :title %>
     <%= f.text_field :title %>
</div>

<div>
    <%= f.label :body %>
    <%= f.text_field :body %>
</div>
<%= f.collection_check_boxes :item_ids, Item.all, :id, :name %>
<% form_tag tests_path do %> 	

	<%= text_field_tag :search %>
	<%= submit_tag "Search" %>

<% end %>
 <%= f.button :submit %>
<% end %>