<%= form_for @user do |f| %>
  <div>
    <%= f.label :username %>
    <%= f.text_field :username %>
  </div>
  <div>
    First name:
    <%= f.text_field :fname %>
  </div>
  <div>
    Last name:
    <%= f.text_field :lname %>
  </div>
  <div>
    <%= f.label :email %>
    <%= f.text_field :email %>
  <div>
    <%= f.label :image %>
    <%= f.file_field :image%>
  </div>
  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <div>
    <%= f.label :password_confirmation %>
    <%= f.password_field :password_confirmation %>
  </div>
  <%= f.submit "Submit these changes!" %>
<% end %>
<%= link_to "Back to the list!", users_path %>